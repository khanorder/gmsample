{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "suppressImplicitAnyIndexErrors": true,
    "ignoreDeprecations": "5.0",
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@defines/*": ["src/defines/*"],
      "@hooks/*": ["src/hooks/*"],
      "@server/*": ["src/server/*"],
      "@session/*": ["src/session/*"],
      "@controllers/*": ["src/server/controllers/*"],
      "@db/*": ["src/server/db/*"],
      "@ngel/*": ["src/ngel/*"],
      "@ngeldata/*": ["src/ngel/data/*"],
      "@ngelscripts/*": ["src/ngel/scripts/*"],
      "@store/*": ["src/store/*"],
      "@helpers/*": ["src/helpers/*"],
      "@sagas/*": ["src/sagas/*"],
      "@styles/*": ["src/styles/*"],
      "@public/*": ["public/*"],
      "@packets/*": ["src/packets/*"],
    }
  },
  "include": ["next-env.d.ts", "src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules", "Certification"]
}